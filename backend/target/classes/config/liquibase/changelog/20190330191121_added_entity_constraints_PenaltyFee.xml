<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity PenaltyFee.
    -->
    <changeSet id="20190330191121-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="departament_id"
                                 baseTableName="penalty_fee"
                                 constraintName="fk_penalty_fee_departament_id"
                                 referencedColumnNames="id"
                                 referencedTableName="department"/>

        <addForeignKeyConstraint baseColumnNames="penalty_fee_type_id"
                                 baseTableName="penalty_fee"
                                 constraintName="fk_penalty_fee_penalty_fee_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="penalty_fee_type"/>

    </changeSet>
</databaseChangeLog>
