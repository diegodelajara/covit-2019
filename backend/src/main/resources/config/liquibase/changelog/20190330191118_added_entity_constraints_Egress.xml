<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Egress.
    -->
    <changeSet id="20190330191118-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="provider_id"
                                 baseTableName="egress"
                                 constraintName="fk_egress_provider_id"
                                 referencedColumnNames="id"
                                 referencedTableName="provider"/>

        <addForeignKeyConstraint baseColumnNames="department_id"
                                 baseTableName="egress"
                                 constraintName="fk_egress_department_id"
                                 referencedColumnNames="id"
                                 referencedTableName="department"/>

        <addForeignKeyConstraint baseColumnNames="tower_id"
                                 baseTableName="egress"
                                 constraintName="fk_egress_tower_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tower"/>

    </changeSet>
</databaseChangeLog>
